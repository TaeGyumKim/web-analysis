function openAgreeLayer_3(objSourceBtn) {
	//removeOverlapDiv('x-window-KCB-3');
	//popup.openDialog('x-window-KCB-3', 710, 580, false);
	var strUrl = "/jsp/cmn/overlay/cvvLayer.jsp";
