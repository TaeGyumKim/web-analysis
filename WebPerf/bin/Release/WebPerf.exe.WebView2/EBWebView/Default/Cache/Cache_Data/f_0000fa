	if(fnIsEnLocale()) strUrl = "/jsp/cmn/overlay/cvvLayer_EN.jsp";
	
	var objData = null;
	fnDoTransaction(strUrl, objData, function(strHTML){
	      
	        layPop("x-window-agree-3", strHTML, objSourceBtn);
	    });
}

