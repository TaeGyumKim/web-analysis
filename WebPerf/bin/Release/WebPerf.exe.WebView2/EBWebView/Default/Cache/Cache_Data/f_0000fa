/**
* <pre>
* desc   : 국내전용은    카드는 맨앞자리가 9(BC Local),
* 		   국내외겸용은 카드는 맨앞자리가 4(BC VISA), 5(BC Master)임
*		   (내맘대로 서비스 신청 연회비 계산)
*
* </pre>
*/

annualFeeCount = function(design_cd, serviceChk, cdNo){
	var annualFee = "";
	if(design_cd == '02373' || design_cd == '02374'){	//플래티넘카드
		annualFee = "80,000";
	}else if (cdNo.substring(0, 1) == '9') {	//국내용카드
		if (serviceChk == '2')
			annualFee = "2,000";
		else if(serviceChk == '4')
			annualFee = "7,000";
		else if(serviceChk == '6')
			annualFee = "17,000";
		else
			annualFee = "32,000";
	} else {		//국내외겸용
		if (serviceChk == '2')
			annualFee = "5,000";
		else if(serviceChk == '4')
			annualFee = "10,000";
		else if(serviceChk == '6')
			annualFee = "20,000";
		else
			annualFee = "35,000";
	}

	return annualFee;
}

