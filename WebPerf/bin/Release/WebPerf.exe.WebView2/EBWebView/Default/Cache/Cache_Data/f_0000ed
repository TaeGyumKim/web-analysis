var noticeGoView = function(_idx){
	//_idx 게시물 구분번호
	var f = document.getElementById("noticeForm1");
	f.AN_NO.value = _idx;
	goMenuSubmit(f, "/OPP35150001D.web?schID=pcd&mID=OPP35150000D&AN_NO=" + _idx);
}

var alertView = function(seq) {
	var f = document.getElementById("alertForm1");
	document.getElementById("CC_NO").value = seq;
	goMenuSubmit(f, "/OSA51200011D.web?schID=scd&mID=OSA51200011D&CC_NO=" + seq);	
}

