getHanaMyPickCount = function(){

	AjaxOption.progressBarYn = "N";
	AjaxOption.modalProgressBarYn= "N";
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getMyPickCount"
	}

	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBackHanaMyPickCount, AjaxOption);
}


callBackHanaMyPickCount = function(dataMap){
	var hanaPickCount = "";
	var obj = jQuery("#hanaPickCount");
	try {
		if(dataMap.APL_PS_CT != null && dataMap.APL_PS_CT != "" && dataMap.APL_PS_CT != undefined) {
			if(dataMap.APL_PS_CT.StrToNum() > 0){
				jQuery("#hanaNonPickCount").hide();
				obj.show();
				obj.find(".total em").text(dataMap.APL_PS_CT.StrToNum());
			}
		}
	}
	catch(e) { }
}

goToPage = function() {		
	if(FAM_TYPE_GUBUN == "1"){ //본인
		
		getExclMember();	
		
		//회원정보 및 결제금액정보 
		getMemberAddtion();		
		getTitleCard(); 		
		
		//getCashAndLoanInfo();	
		
		
		getSmartOfferBanner('WHP_MAIN_BNR_RT_BT');		
		
		
		getCardServiceMoney();
		
		
		getPoint();
		
		
		getBirthYn();
		
		
		//getMyEvent();
		getMyEvent('main');
		
		
		getHanaMyPickCount();
		
	}else if(FAM_TYPE_GUBUN == "2"){ //가족
		goMenuLink('OPY00200000M.web?schId=pcd&mID=OPY00200000M&_frame=yes');
	
	}else if(FAM_TYPE_GUBUN == "3"){ //선불
		goMenuLink('OPY06002000M.web?schId=pcd&mID=OPY06002000M&_frame=yes');
	
	} 	

	iTracerAF.init(function (data) {
	    jQuery("#ipinsideAfWData2").val(data.wdata);
	    jQuery("#ipinsideAfUKey2").val(data.ukey.replace(/-/g,""));
	    isAFCompletion = true;
	    initEFDSData();
	});
}


//레이어팝업 하루동안 감추기
layerPopupTodayClose = function(layerDiv) {
	setCookie(layerDiv, "no" , "1");
	jQuery("#"+layerDiv).popup("hide");
	
	if ("birthPopUp_Html" == layerDiv) {
		pauseAudio();
	}
	
}

function closeAllMenu(){
	jQuery(function($){
		$('#all_menu').popup('hide');
	});
}

function goEngLink() {
	if(_REAL_MODE == 'R') {
		link = 'https://eng.hanacard.co.kr/';
	} else {
		link = 'https://deng.hanacard.co.kr:1443/';
	}
	window.open(link, "_blank");
}

