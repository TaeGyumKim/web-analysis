}

callBacPoint = function(dataMap){
	
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! callBacPoint]: "+dataMap.RESPONSE_MESSAGE);
			
		}else{
			
			var U_PSB_PT_003_TMP = ( undefined == dataMap.U_PSB_PT_003 || null == dataMap.U_PSB_PT_003 || "" == dataMap.U_PSB_PT_003) ? "0" : dataMap.U_PSB_PT_003;
			jQuery("#u_psb_pt_003").html(U_PSB_PT_003_TMP+"<em>포인트</em>");
			
		}	
		
		
	}catch(e){
		//alert("[개발테스트] callBacPoint 에러");
		
	}
	
}


