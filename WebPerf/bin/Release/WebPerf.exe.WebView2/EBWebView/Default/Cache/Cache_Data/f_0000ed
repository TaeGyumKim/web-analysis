	<script language="Javascript" type="text/javascript">
		jQuery(function($){
			$.fn.myBenefitSlide = function(){
				$.each(this, function(i,v){
					$(v).carouFredSel({
						responsive:false,
						firstLoadChk :true,
						direction:'left',
						circular:true,
						infinite:true,
						//items:1,
						auto:false,
						prev:false,
						next:false,
						pagination:".pagn_s",
					});
				});
				return this;
			};
			$('.benefit_spec_slide').myBenefitSlide()
		})
			
		
		function setCookie( name, value, expirehours ) { 
			var todayDate = new Date(); 
			todayDate.setHours( todayDate.getHours() + expirehours ); 
			document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" 
		} 

		function closeWin() { 	
			if(document.getElementById("chk_today").checked){
				setCookie( "promoPopYn", "N" , 3 ); 
			}
			jQuery('#promo_pop').popup('hide');
		}
        
	</script>


