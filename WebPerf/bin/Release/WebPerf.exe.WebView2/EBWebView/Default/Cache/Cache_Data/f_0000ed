/* 접근성 이미지 alt 처리 추가 2018.08.27*/
jQuery(function($){
	/* img Alt */
	$.fn.imgAlt = function(){
		$.each(this, function(i,v){
			var s = $(v).attr('data-title')
			$(v).attr('alt', s).removeAttr('data-title');
		});
	};
	$('.img_alt').imgAlt();

});

<!-- 하나Pay 앱 로그인 -->
var limitTime = 0;


//1Q로그인 확인 폴링
function authVerifyChk() {
	
	if (limitTime <= 0) {
		alert("인증시간이 초과되었습니다. 다시 시도해 주세요.");
		return;
	}

	var reqMap = {
		targetMethod : "authVerifyChk"

	};
	executeAction("/OQW10000000M.ajax", eval(reqMap), function(dataMap) {

		if (dataMap.RESPONSE_MESSAGE != null
				|| dataMap.RESPONSE_MESSAGE != undefined) {
			extExceptionAlert(dataMap);
			return;
		} else {

			// 성공
			if (dataMap.RESULT == "SUCCESS") {

				if (dataMap.AUTH_VERIFY_CODE == "00") {

					// 인증성공 후 로그인 시도
					chLogin();
				}else if(dataMap.AUTH_VERIFY_CODE == "99"){
	       			alert("하나Pay앱 앱카드 인증 유효시간이 초과하였습니다. 다시 시도해 주세요.");
	       			clear1QLogin();
				} else if (dataMap.AUTH_VERIFY_CODE == "9999") {
					// 오류
					alert("잠시 후 다시 이용해 주세요.");
					clear1QLogin();
				} else {
					// 인증 전
										
				}
				
				

			} else {
				alert(dataMap.RT_MSG);
				clear1QLogin();
			}

		}

	}, AjaxOption);

}


//2025.07 접근성 시간 연장
function extTime1QLogin() {
	var reqMap = {
			targetMethod : "extTime1QLogin",
			EASN_LGN_CTF_ID : EASN_LGN_CTF_ID

		};
		executeAction("/OQW10000000M.ajax", eval(reqMap), function(dataMap) {
			if (dataMap.RESPONSE_MESSAGE != null
					|| dataMap.RESPONSE_MESSAGE != undefined) {
				extExceptionAlert(dataMap);
				return;
			} else {

				// 성공
				if (dataMap.RESULT == "SUCCESS") {
					clearInterval(appLoginTimer);

					limitTime = Number(dataMap.VALID_TIME); // 유효시간(분)          		        						
					limitTime = limitTime * 60; // 유효시간 분 > 초로 표시
					
					var min = "";
					var sec = "";
					jQuery("#1QBtn").show();
					jQuery("#btn_timeadd").show();
					jQuery("#1QArea").show();
					
					// 1초마다 유효시간(초)를 감소시킨다.
					appLoginTimer = setInterval(function() {

						min = parseInt(limitTime/60);
						sec = parseInt(limitTime%60);
						
						// 유효시간이 끝났다
						if (limitTime <= 0) {

							clear1QLogin();

						} else {
							jQuery("#timer").html(min+"분 "+sec+"초");
						}
						
						limitTime--;
					}, 1000);

				}
				// 실패
				else {
					alert(dataMap.RT_MSG);
					clear1QLogin();
				}

			}

		}, AjaxOption);
}

//하나Pay앱 로그인 요청
var appLoginTimer;
var qrWidth = 300;
var qrHeight = 300;
var EASN_LGN_CTF_ID;
function init1QLogin() {
	var reqMap = {
		targetMethod : "init1QLogin",
		qrWidth : qrWidth,
		qrHeight : qrHeight

	};
	executeAction("/OQW10000000M.ajax", eval(reqMap), function(dataMap) {

		if (dataMap.RESPONSE_MESSAGE != null
				|| dataMap.RESPONSE_MESSAGE != undefined) {
			extExceptionAlert(dataMap);
			return;
		} else {

			// 성공
			if (dataMap.RESULT == "SUCCESS") {
				clearInterval(appLoginTimer);
				//QR코드
				jQuery('#qrImg').attr("src",'data:application/octet-binary;base64,'+ dataMap.QR_IMG); //QR 이미지
				jQuery("#qrImg").show();
				jQuery("#code").html(dataMap.EASN_LGN_CTF_ID);
				EASN_LGN_CTF_ID = dataMap.EASN_LGN_CTF_ID;

				limitTime = Number(dataMap.VALID_TIME); // 유효시간(분)          		        						
				limitTime = limitTime * 60; // 유효시간 분 > 초로 표시
				
				var min = "";
				var sec = "";
				
				jQuery("#1QBtn").show();
				jQuery("#btn_timeadd").show();
				jQuery("#1QArea").show();

				// 1초마다 유효시간(초)를 감소시킨다.
				appLoginTimer = setInterval(function() {

					min = parseInt(limitTime/60);
					sec = parseInt(limitTime%60);
					
					// 유효시간이 끝났다
					if (limitTime <= 0) {

						clear1QLogin();

					} else {
						jQuery("#timer").html(min+"분 "+sec+"초");
					}
					
					limitTime--;
				}, 1000);

			}
			// 실패
			else {
				alert(dataMap.RT_MSG);
				clear1QLogin();
			}

		}

	}, AjaxOption);
}

function clear1QLogin() {
	clearInterval(appLoginTimer);
	jQuery("#timer").html("인증시간초과");
	jQuery("#1QBtn").hide();
	jQuery("#btn_timeadd").hide();

}
// 로그인 전처리
function chLogin() {

	var reqMap = {
		targetMethod : "chLogin"

	};
	executeAction("/OQW10000000M.ajax", eval(reqMap), function(dataMap) {

		if (dataMap.RESPONSE_MESSAGE != null
				|| dataMap.RESPONSE_MESSAGE != undefined) {
			extExceptionAlert(dataMap);
			return;
		} else {
			// 성공
			if (dataMap.RESULT == "SUCCESS") {
				doSubmit3();
			}
			// 실패  
			else {
				alert(dataMap.RT_MSG);
			}

		}

	}, AjaxOption);

}


doSubmit3 = function() {
	jQuery(".loading").show();
	AjaxOption.encription = true;
	AjaxOption.progressBarYn = "N";
	AjaxOption.modalProgressBarYn = "N";
	AjaxOption.encodingYn = "N";

	var reqMap = { targetMethod : "autoLogin" }
	executeAction("/OQW10000000M.ajax", checkDataMap(reqMap), autoLoginCallBackFunction, AjaxOption);		
}

function autoLoginCallBackFunction(dataMap) {
	if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined) {
		if(dataMap.ERROR_CODE != null || dataMap.ERROR_CODE != undefined) {
			jQuery(".loading").hide();
			extExceptionAlert(dataMap);
		}
	}else if(dataMap.NOT_SESSION == "Y") {
		goMenuLink("OPM05000000C.web?schID=pcd&mID=OPM05000000C");
	}else {
		var FAM_TYPE_GUBUN = dataMap.FAM_TYPE_GUBUN;

		if(FAM_TYPE_GUBUN == "1"){ //본인
			goMenuLink("OPY00000000M.web?schID=pcd&mID=OPY00000000M");
		}else if(FAM_TYPE_GUBUN == "2"){ //가족
			goMenuLink("OPY00200000M.web?schId=pcd&mID=OPY00200000M");
		}else if(FAM_TYPE_GUBUN == "3"){ //선불
			goMenuLink("OPY06002000M.web?schId=pcd&mID=OPY06002000M");
		}
	}
}

function close1QPop(){
	jQuery('#1QArea').hide();
	jQuery('#1QPayLoginPop').popup('hide');
	jQuery('#1QBtn').hide();
	clearInterval(appLoginTimer);
	jQuery("#timer").html("3분 0초");
}

function open1QPop(){
	init1QLogin();
	jQuery('#1QPayLoginPop').popup('show');
}
