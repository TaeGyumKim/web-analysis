								<i class="s1"></i><i class="s2"></i>
							</li>
							<li class="c5">
								<i class="s1"></i><i class="s2"></i><i class="s3"></i><i class="s4"></i>
							</li>
						</ul>
					</div>
					
				</div>
			</article>
			<!-- //본문 -->
		</div>
		<!-- //modal_body -->
		<!-- modal_footer -->
		<div class="modal_footer">
			<div class="btn_sound">
				<audio src="/hana/images/contents/birth_audio.mp3" loop id="sound_birth"></audio><!-- 추후 생일축하 음악으로 변경 -->
				<a href="javascript:void(0);" onClick="playAudio();" class="audio_play">음악재생</a>
