		initEFDsData(siteMode,'Y');
	
	
}

function afterInit(parmWData,parmNData,parmUData){
	$_astxu.log('[afterInit]' + parmWData);
	parmWData1 = parmWData;
	parmNData1 = parmNData;
	parmUData1 = parmUData;
	AjaxOption.encription = true;
	var reqMap = {
		targetUri : "OPH30000000N.web",
		ipinsideData : parmWData,
		ipinsideNAT : parmNData,
		ipinsideCOMM : parmUData,
		ipinsideAfWData : jQuery("#ipinsideAfWData2").val(),
		ipinsideAfUKey : jQuery("#ipinsideAfUKey2").val()
	}
	executeAction("OCM25900000M.ajax", checkDataMap(reqMap), callbackAfterGet2, AjaxOption);
}
function callbackAfterGet2() {
	ASTXInit('FALSE', 'TRUE');
	initializedNFilter2();
	spider.extJS.loadingLayer.close();
}

//이중로그인 ERROR 체크 팝업
duplicateLoginCheckPopup = function(){
	
    var dupYN = 'N';
    
    if(dupYN == 'Y') {
        i18nExtAlert('다른 스마트폰 또는 PC에서<br/>고객님의 정보로 로그인되었습니다.<br/>보안상 동일인이 여러기기에 동시에 로그인 접속을 제한하오니, 불편하시더라도 다시 한번 로그인하여 주시기 바랍니다.', '세션이 종료되었습니다.');
        goMenuLink('/OCM90000000N.web');
    }
}


//2024.08 접근 임시 작성
function passwordClick(){
	document.getElementById('PASSWORD').click();
}

// ID PW 로그인
doSubmit1 = function() {
	var f = document.getElementById("loginForm");
	var USER_ID = document.getElementById("USER_ID").value.trim();
	var PASSWORD = document.getElementById("PASSWORD").value.trim();
	
	if(USER_ID == "") {
        i18nExtAlert("ID는 필수 입력 항목 입니다.");
        return;
    }	
	if(PASSWORD == "") {
        i18nExtAlert("PASSWORD는 필수 입력 항목 입니다.");
        return;
    }		
	USER_ID = USER_ID.toUpperCase();
	
	var nEncryptValue = nFilterEncrypted();	
	//var nEncryptValue = nshc.encrypted();	//20240823
            document.getElementById("_nFilterEncryptValue").value = nEncryptValue;
  	
  	if( true == $_astxu.isMacLinuxOS() || true == isVMEnv()) {
		AjaxOption.encription = true;
		AjaxOption.encodingYn = "N";
		
		var reqMap = {
				targetMethod 		: "login",
				USER_ID				: USER_ID,
				PASSWORD 			: PASSWORD,      	    		
     	    	_nFilterEncryptValue : nEncryptValue
 	    	}
		
		executeAction("OCM000000M.ajax", checkDataMap(reqMap), loginCallBackFunction, AjaxOption);		
		
	}else{
		$ASTX2.getE2EDataIDs(				
			["PASSWORD"],
			
			function onSuccess(data) {
			
			AjaxOption.encription = false;
			AjaxOption.progressBarYn = "N"; 
			AjaxOption.modalProgressBarYn= "Y";

			data.targetMethod = "login";				
			data.USER_ID = USER_ID;
			data.PASSWORD = "";
			data._nFilterEncryptValue = nEncryptValue;
    				
			
			executeAction("OCM000000M.ajax", checkDataMap(data), loginCallBackFunction, AjaxOption);
			

			}, 
 			function onFailure() {
 				
 				i18nExtAlert("조회중 에러가 발생하였습니다.");
 		      	//ASTX E2E대상 필드 관련 초기화. AJAX거래시 이 초기화 안해주면 기존입력값으로 유지됨
 			    $ASTX2.clearE2EText(document.getElementById("PASSWORD"));
 				$_astxu.log('ASTX.getE2EData() failure: errno='+$ASTX2.getLastError());
 			});
		}  	    
}

//ID PW 로그인 콜백함수
function loginCallBackFunction(dataMap){
    if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){        
        
        if(dataMap.ERROR_CODE != null || dataMap.ERROR_CODE != undefined){
        	if(dataMap.ERROR_CODE == "CME00004"){ //아이디 또는 비밀번호 오류        		
        		jQuery('#error01').popup('show');	
        	}else if(dataMap.ERROR_CODE == "CME00001"){ //비밀번호 5회 오류        		
        		jQuery('#error02').popup('show');
        	}else if(dataMap.ERROR_CODE == "CME00171") {
        		jQuery('#error06').popup('show');
        	}else if(dataMap.ERROR_CODE == "CME00162"){
        		jQuery('#CustInfOutfChkErr').popup('show');
	}else if(dataMap.ERROR_CODE == "CME00205"){
        		jQuery('#CustInfOutfChkErr2').popup('show');
        	}else{        		
        		i18nExtAlert(dataMap.RESPONSE_MESSAGE);
        	}        	
        }
      	//ASTX E2E대상 필드 관련 초기화. AJAX거래시 이 초기화 안해주면 기존입력값으로 유지됨
        $ASTX2.clearE2EText(document.getElementById("PASSWORD")); 
    }else{
    	FAM_TYPE_GUBUN = dataMap.FAM_TYPE_GUBUN;
    	PSWD_CHANGE_YN = dataMap.PSWD_CHANGE_YN;
    	CDD_ERROR_YN = dataMap.CDD_ERROR_YN;
    	CDD_IS_YN = dataMap.CDD_IS_YN;
    	sendEFDS('idpw');		
    }
}



doSubmit2 = function() {
	var f = document.getElementById("loginForm");
	
	var signData  = "login=certLogin";
	var certType = "login";
	
	Delfino.login(signData, {complete:completeCB, context:certType});
}


var ttmp1 = "";
var ttmp2 = "";
var ttmp3 = "";



function completeCB(result,context){
	
	if (result.status==0) {
		ttmp1 = result;
		return;
	}
	
	if (result.status==1) {
		ttmp1 = result;
		AjaxOption.encription = true;
		
		PKCS7 = result.signData; //로그인 페이지에서 사용
		VID_RANDOM = result.vidRandom; //로그인 페이지에서 사용
		
		var reqMap = {
		 		targetMethodID : "signVerify",
		    	PKCS7          : result.signData,
		    	VID_RANDOM     : result.vidRandom,
		    	CertType	   : context,
		    	MenuURI : "OPH30000000N.web"
		}				
		executeAction("HsciCertLogPN.ajax", eval(reqMap), signLoginCallBack, AjaxOption);
	}
}


//공인인증 로그인 콜백함수			
function signLoginCallBack(dataMap){	
	ttmp2 = dataMap;
    if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){
        if(dataMap.ERROR_CODE != null || dataMap.ERROR_CODE != undefined){
            if(dataMap.ERROR_CODE == "CME00093"){ //등록되지 않은 인증서
                jQuery('#error03').popup('show');
            }else{
                i18nExtAlert(dataMap.RESPONSE_MESSAGE);
            }
        }
    }else{
    	AjaxOption.encription = true;			 			
		
    	var reqMap = {
				targetMethod : "login",
		    	PKCS7        : PKCS7,
		    	VID_RANDOM   : VID_RANDOM			    	
		}
		
		executeAction("OCM000000M.ajax", eval(reqMap), signLoginCallBack2, AjaxOption);
		
    }
}

function signLoginCallBack2(dataMap){	
	ttmp3 = dataMap;
    if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){        
        
        if(dataMap.ERROR_CODE != null || dataMap.ERROR_CODE != undefined){
        	if(dataMap.ERROR_CODE == "CME00093"){ //등록되지 않은 인증서        		
        		jQuery('#error03').popup('show');	        	
        	}else if(dataMap.ERROR_CODE == "CME00171") {
        		jQuery('#error06').popup('show');
        	}else{        		
        		i18nExtAlert(dataMap.RESPONSE_MESSAGE);
        	}        	
        }      	
    }else{    	
    	FAM_TYPE_GUBUN = dataMap.FAM_TYPE_GUBUN;
    	sendEFDS('cert');
    }
}

function sendEFDS(args){	
	AjaxOption.encription = true;			    	    		        
			
	var reqMap = {
			targetUri : "OCM000000M.ajax",
			ipinsideData : parmWData1,
			ipinsideNAT : parmNData1,
			ipinsideCOMM : parmUData1,
			ipinsideAfWData : jQuery("#ipinsideAfWData2").val(),
			ipinsideAfUKey : jQuery("#ipinsideAfUKey2").val()    		
	}
	
	executeAction("OCM25900000M.ajax", checkDataMap(reqMap), callbackAfterGet(args) , AjaxOption);	        
}

function callbackAfterGet(args){
	
	var type = args;
	if(type == "cert"){
		submitStateFalse(document.forms['form2']);
    	document.forms['form2'].process = "false";    	
    		
    	goMenuLink('OPH30000000N.web?_frame=no&schID=pcd&mID=OPH30000000C');
	}else{
		if(PSWD_CHANGE_YN == "Y"){
			jQuery(".processing").hide();
    		jQuery('#error04').popup('show'); //비밀번호 변경 대상자
    	}else if(CDD_ERROR_YN == "N" && CDD_IS_YN == "Y"){
    		jQuery(".processing").hide();
    		document.getElementById("CDD_IS_YN").value = "Y";
    		jQuery('#error05').popup('show'); //CDD 재이행 대상자      	    		 		
    	}else{    		
    		goMenuLink('OPH30000000N.web?_frame=no&schID=pcd&mID=OPH30000000C');   		
    	}		
	}
	
	
}



updateDisAgreePswd = function(){
		jQuery('#error04').popup('hide');
       //연장날짜 세팅 작업
       if( true == $_astxu.isMacLinuxOS() || true == isVMEnv()) {
		// MacLinuxOS
       	AjaxOption.encription = false;
       	var reqMap = {
       			targetMethod		: "updateAgrePswd"       			
       	}
       	executeAction("OPY35450001D.ajax", eval(reqMap) ,callBackupdateAgrePswd, AjaxOption);
	} else {
		$ASTX2.getE2EDataIDs(
			[],
			function onSuccess(data) {		
				AjaxOption.encription = false;
				AjaxOption.progressBarYn = "N";
				AjaxOption.modalProgressBarYn= "Y";
		
				data.targetMethod 	= "updateAgrePswd";
		
				executeAction("OPY35450001D.ajax", eval(data), callBackupdateAgrePswd, AjaxOption);
			}, 
			function onFailure() {
				i18nExtAlert("E2E 오류 발생");
				$_astxu.log('ASTX.getE2EData() failure: errno='+$ASTX2.getLastError());
			}
		);	
	}     
}

callBackupdateAgrePswd = function(dataMap){
	if( undefined != dataMap.RESPONSE_MESSAGE && null != dataMap.RESPONSE_MESSAGE) {
		if(!ajaxIsLogin(dataMap)) {
		    ajaxLoginException(dataMap);
		} else {
		    extExceptionAlert(dataMap);
		}
		return;
	}
	goMenuLink('OPH30000000N.web?_frame=no&schID=pcd&mID=OPH30000000C');
}

function goPasswordChange(){
	//ASTXgoMenuSubmit(document.forms['loginForm'], 'OPY35450001M.web?schID=pcd&amp;mID=OPY35450001M','FALSE');
	goMenuLink('OPY35450001M.web?schID=pcd&amp;mID=OPY35450001M');
}

function goChangeCDD(){
	document.getElementById("tiles_frame").value = "yes";
	ASTXgoMenuSubmit(document.forms['loginForm'], 'OPY35000000M.web?schID=pcd&mID=OPY35000000T','FALSE');
}

function updateAML(){
	jQuery('#error05').popup('hide');
		// CDD재이행 일자 업데이트
    if( true == $_astxu.isMacLinuxOS() || true == isVMEnv()) {
		// MacLinuxOS
       	AjaxOption.encription = false;
       	var reqMap = {
       			targetMethod		: "updateAML"       			
       	}
       	executeAction("OCM000000M.ajax", eval(reqMap) ,callBackupdateAML, AjaxOption);
	} else {
		$ASTX2.getE2EDataIDs(
			[],
			function onSuccess(data) {		
				AjaxOption.encription = false;
				AjaxOption.progressBarYn = "N";
				AjaxOption.modalProgressBarYn= "Y";
		
				data.targetMethod 	= "updateAML";
		
				executeAction("OCM000000M.ajax", eval(data), callBackupdateAML, AjaxOption);
			}, 
			function onFailure() {
				i18nExtAlert("E2E 오류 발생");
				$_astxu.log('ASTX.getE2EData() failure: errno='+$ASTX2.getLastError());
			}
		);	
	}
}

callBackupdateAML = function(dataMap){
	if( undefined != dataMap.RESPONSE_MESSAGE && null != dataMap.RESPONSE_MESSAGE) {
		if(!ajaxIsLogin(dataMap)) {
		    ajaxLoginException(dataMap);
		} else {
		    extExceptionAlert(dataMap);
		}
		return;
	}
	//goToPage();		
	goMenuLink('OPH30000000N.web?_frame=no&schID=pcd&mID=OPH30000000C');
}



<!--  [OpenWeb] 노현석 가상키패드 적용 0212 [-->
function initializedNFilter2(){
//	nshc.setOnInitStart();
	
	nFilterinputNone();
//	nshc.setCommon(document.getElementById('PASSWORD'), "mode=pcCKpd"); 
//	nshc.setInit();
	setNFilterCommon(document.getElementById('PASSWORD'), "mode=qwerty"); 
	setNFilterInit();
	nFilterAction('PASSWORD','disable','qwerty','btnMouse.gif', 'TRUE');
//	nFilterAction('PASSWORD','disable','pcCKpd','btnMouse.gif', 'TRUE');
}

getExclMember = function() {
	var reqMap = {
		targetMethod : "getExclMemeber"
	}
	
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBackExclMemeber, AjaxOption);
}


callBackExclMemeber = function(dataMap)
{
	try{
		
		if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){
			//i18nExtAlert("[개발 테스트 에러! getExclMember]: "+dataMap.RESPONSE_MESSAGE);
			
	    }else{
	    	var mc_user_name = "";
			var mc_grade = "";
			var isVip = true;
			
			
			
			//[Day2] 거래 변경으로 수정: AS-IS용어 사용하지 않음(남재현 D) [TO-BE=> 01~03: VVIP, 04: 퍼스트, 05: 로열, 06: 프리미어, 07: 클래식, 08~10:블루]
			if(dataMap.VIP_GDC_NM == "01" || dataMap.VIP_GDC_NM == "02" || dataMap.VIP_GDC_NM == "03"){	
				mc_grade = '<img src="/hana/images/contents/ico_vip_info01.png" alt="VVIP">';
			}
			else if(dataMap.VIP_GDC_NM == "04"){ 
				mc_grade = '<img src="/hana/images/contents/ico_vip_info02.png" alt="VIP 퍼스트">';
				
			}
			else if(dataMap.VIP_GDC_NM == "05"){	
				mc_grade = '<img src="/hana/images/contents/ico_vip_info03.png" alt="VIP 로열">';
				
			}
			else if(dataMap.VIP_GDC_NM == "06"){ 
				mc_grade = '<img src="/hana/images/contents/ico_vip_info04.png" alt="VIP 프리미어">';
				
			}
			else if(dataMap.VIP_GDC_NM == "07"  || dataMap.VIP_GDC_NM == "08" ){ 
				mc_grade = '<img src="/hana/images/contents/ico_vip_info05.png" alt="VIP 클래식">';
				
			}
			else{
				isVip = false;
				
			}
			
			if(!isVip){
				jQuery("#mc_box_area_login .s2").hide();
				jQuery("#mc_box_area_login .s3").hide();
				
			}else{
				jQuery("#mc_box_area_login .s2 span").html(mc_grade)
				
				jQuery("#mc_box_area_login .s2").show();
				jQuery("#mc_box_area_login .s3").show();
				
				getSmartOfferBanner('WHP_MAIN_BNR_LF_BT');		
				
			}	
	    }
	}
	catch(e)
	{
		//alert("[개발테스트] getExclMemeber 에러");
	}
}


//회원 정보 로딩 시작=========================================================
getMemberAddtion = function(type){
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getMemberAddtion_new" // [PLCC] targetMethod 변경 (getMemberAddtion > getMemberAddtion_new)
	}	
	// [PLCC] callBack 변경 (callBacMemberAddtion > callBacMemberAddtion_new)
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBacMemberAddtion_new, AjaxOption);
}

callBacMemberAddtion = function(dataMap)
{
	
	
  	try{
  		
		if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){
			//i18nExtAlert("[개발 테스트 에러! getMemberAddtion]: "+dataMap.RESPONSE_MESSAGE);
			
	    }else{
	    	var limitHtml = "";
	  		
	  		var TOT_HOR8 = "";	
	  		var TOT_HOR8_2 = ""; 
	  		
	  		var isLimitSet_YN = "N";	
	  		
	  		  if(dataMap.BILL_2ND_YN == "Y"){ //2건
				
	  			///// 결제 예정금액/금액표시_하나
	        	// [2016.04.18] 금액컬럼  BLL_STT_AM-> STT_PL_AM 로 변경 / 결제일컬럼 HLD_B_REAL_STT_DT -> DISPLAY_TODAY 변경
	        	// [2016.04.27] 추가적용 [SR-160407-DEV-005]_마이페이지 결제금액 기준 변경   STT_PL_AM -> TOT_HOR8
	    		if(Number(dataMap.TOT_HOR8) == 0 || dataMap.TOT_HOR8=="" || dataMap.TOT_HOR8==undefined) {
	    			TOT_HOR8 = "0";
	    			
	      		}else{
	      			TOT_HOR8 = getInsComma(Number(dataMap.TOT_HOR8));
	      			
	      		}
	  			  
	    		///// 결제 예정금액/금액표시_외환
	      	    // [2016.04.27] 추가적용 [SR-160407-DEV-005]_마이페이지 결제금액 기준 변경   STT_PL_AM2 -> TOT_HOR8_2
	   			if(Number(dataMap.TOT_HOR8_2) == 0 || dataMap.TOT_HOR8_2=="" || dataMap.TOT_HOR8_2==undefined)	{
	   				TOT_HOR8_2 = "0";          
	   				
	   			}else{
	   				TOT_HOR8_2 = getInsComma(Number(dataMap.TOT_HOR8_2));
	   				
	   			}
	  			  
	   			jQuery("#mc_box_area_dt1_login").html("하나카드");
	      	  	jQuery("#mc_box_area_em1_login").html("<em>"+TOT_HOR8+"</em>원");
	      	  	
	      	  	jQuery("#mc_box_area_dt2_login").html("구.외환카드");
	    	  	jQuery("#mc_box_area_em2_login").html("<em>"+TOT_HOR8_2+"</em>원");
	  			  
	    	  	
	    	}else{  //단건
	    		
	    		isLimitSet_YN = "Y";	
	    		
	    		// [2016.04.18] 금액컬럼  BLL_STT_AM-> STT_PL_AM 로 변경 / 결제일컬럼 HLD_B_REAL_STT_DT -> DISPLAY_TODAY 변경
	      	    // [2016.04.27] 추가적용 [SR-160407-DEV-005]_마이페이지 결제금액 기준 변경   STT_PL_AM -> TOT_HOR8
	    		if(Number(dataMap.TOT_HOR8) == 0 || dataMap.TOT_HOR8=="" || dataMap.TOT_HOR8==undefined)	{
	    			TOT_HOR8 = "0";
	    			
	  			}else{
	  				TOT_HOR8 = getInsComma(Number(dataMap.TOT_HOR8));
	  			
	  			}
	      	    jQuery("#mc_box_area_dt1_login").html("<a href='javascript:void(0);' class='btn_plus' onclick='goMenuLink(\"OPY00150000M.web?schID=pcd&mID=OPY00150000M\"); return false;'>결제예정금액</a>");
	      	  	jQuery("#mc_box_area_em1_login").html("<em>"+TOT_HOR8+"</em>원");
	      	  	
	      	  	
	      	  	
	      	  	var var_BIL_UNIT_DC3 = (dataMap.BIL_UNIT_DC3 == undefined)? "" : dataMap.BIL_UNIT_DC3.trim(); //통합
				var var_BIL_SEQ3 = (dataMap.BIL_SEQ3 == undefined)? "" : dataMap.BIL_SEQ3.trim(); //통합
				var var_TOT_HOR8_10_YN = (dataMap.TOT_HOR8_10_YN == undefined)? "" : dataMap.TOT_HOR8_10_YN.trim();
				
				if(var_BIL_UNIT_DC3 != null && var_BIL_UNIT_DC3 != "" && var_BIL_SEQ3 != null && var_BIL_SEQ3 != "") {	//통합고객이면 , 
					
					if(var_TOT_HOR8_10_YN == "Y"){	// 구.하나, 구.외환 나누어서 표기함.
						
						var var_TOT_HOR8_10 = (dataMap.TOT_HOR8_10 == undefined)? 0 : dataMap.TOT_HOR8_10.trim();	//구.외환 결제예정금액
						
						if(Number(var_TOT_HOR8_10) != 0){		//금액이 0이 아닐경우 구.외환 노출
							
	      	  				isLimitSet_YN = "N";	
	      	  				
		      	  			jQuery("#mc_box_area_dt1_login").html("하나카드");
		    	      	  	jQuery("#mc_box_area_em1_login").html("<em>"+TOT_HOR8+"</em>원");
		    	      	  	
		    	      	  	jQuery("#mc_box_area_dt2_login").html("구.외환카드");
		    	    	  	jQuery("#mc_box_area_em2_login").html("<em>"+getInsComma(Number(var_TOT_HOR8_10))+"</em>원");
	      	  			}
					}
				}
	    	}
	  		
	  		getMemLimit(isLimitSet_YN);	
	    }

  	}catch(e){
  			
  			//alert("[개발테스트] callBacMemberAddtion1:"+e);
  	}  		
}
callBacMemberAddtion_new = function(dataMap)
{
	try{
  		
		if(dataMap.RESPONSE_MESSAGE != null || dataMap.RESPONSE_MESSAGE != undefined){
			//i18nExtAlert("[개발 테스트 에러! getMemberAddtion]: "+dataMap.RESPONSE_MESSAGE);
	    }
		else {
			var isLimitSet_YN = "N";	
			
			var data = "";
			if(dataMap.BILS != null && dataMap.BILS != undefined) {
				data = dataMap.BILS.data;
				if(data.length > 0 ) {
					for(var i = 0; i < data.length; i++) {
						if(i == 0) {

							if( data.length == 1) {
								isLimitSet_YN = "Y";
								jQuery("#mc_box_area_dt1_login").html("<a href='javascript:void(0);' class='btn_plus' onclick='goMenuLink(\"OPY00150000M.web?schID=pcd&mID=OPY00150000M\"); return false;'>결제예정금액</a>");
							}
							else {
								isLimitSet_YN = "N";
								if( data[i].BIL_UNIT_DC == "2" || ( data[i].BIL_UNIT_DC == "3" && data[i].TOT_HOR8_10_YN == "Y") ) {
									jQuery("#mc_box_area_dt1_login").html("구.외환카드");
								}
								else if( data[i].BIL_UNIT_DC == "P") {										
									jQuery("#mc_box_area_dt1_login").html(data[i].PLCC_BIL_UNIT_NM);
								}
								else {										
									jQuery("#mc_box_area_dt1_login").html("하나카드");
								}
							}
							
							if( data[i].TOT_HOR8==undefined || data[i].TOT_HOR8=="" || Number(data[i].TOT_HOR8) == 0) {
								jQuery("#mc_box_area_em1_login").html("<em>0</em>원");
								_TOT_HOR8_1 = 0;
							}
							else {
								jQuery("#mc_box_area_em1_login").html("<em>"+ getInsComma(Number(data[i].TOT_HOR8)) +"</em>원");
								_TOT_HOR8_1 = Number(data[i].TOT_HOR8);
							}
						}
						else if(i == 1) {

							if( data[i].BIL_UNIT_DC == "2" || ( data[i].BIL_UNIT_DC == "3" && data[i].TOT_HOR8_10_YN == "Y") ) {
								jQuery("#mc_box_area_dt2_login").html("구.외환카드");
							}
							else if( data[i].BIL_UNIT_DC == "P") {										
								jQuery("#mc_box_area_dt2_login").html(data[i].PLCC_BIL_UNIT_NM);
							}
							else {
								jQuery("#mc_box_area_dt2_login").html("하나카드");
							}								

							if( data[i].TOT_HOR8==undefined || data[i].TOT_HOR8=="" || Number(data[i].TOT_HOR8) == 0) {
								jQuery("#mc_box_area_em2_login").html("<em>0</em>원");
							}
							else{
								jQuery("#mc_box_area_em2_login").html("<em>"+ getInsComma(Number(data[i].TOT_HOR8)) +"</em>원");									
							}
						}
					}
				}
			}			
			if( null == data || undefined == data || data.length == 0) {
				isLimitSet_YN = "Y";
				jQuery("#mc_box_area_dt1_login").html("<a href='javascript:void(0);' class='btn_plus' onclick='goMenuLink(\"OPY00150000M.web?schID=pcd&mID=OPY00150000M\"); return false;'>결제예정금액</a>");
				jQuery("#mc_box_area_em1_login").html("<em>0</em>원");
			}			

			
			getMemLimit(isLimitSet_YN);	
		}
	}
	catch(e){  			
  			//alert("[개발테스트] callBacMemberAddtion1:"+e);
  	} 
}
//=========================================================회원 정보 로딩 끝




getMemLimit = function(bill_2nd_yn){
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getMemberLimit",
	}
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), function(dataMap){ callBackMemLimit(dataMap,bill_2nd_yn) }, AjaxOption);
}

callBackMemLimit = function(dataMap, bill_2nd_yn){
	try{

		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			
			jQuery("#mc_cash_area_login li:first-child div").html("<strong>이용가능금액</strong>0<em>원</em><em class='after'>/</em>");	
      	  	jQuery("#mc_cash_area_login li:nth-child(2) div").html("<strong>이용가능금액</strong>0<em>원</em><em class='after'>/</em>");	
      	  	
			if("Y" == bill_2nd_yn){
				jQuery("#mc_box_area_dt2_login").html("<a href='javascript:void(0);' class='btn_plus' onclick='goMenuLink(\"OPY00350100M.web?schID=pcd&mID=OPY00350100M\"); return false;'>총 이용한도</a>");
	      	  	jQuery("#mc_box_area_em2_login").html("<em>0</em>원");
			}
			
		}else{
			var 통합한도var 	   = (dataMap.TLIM_AM == undefined || dataMap.TLIM_AM == "") ? "error" : Number(dataMap.TLIM_AM);
			var 현금서비스잔여var = (dataMap.APR_PSB_AM5 == undefined || dataMap.APR_PSB_AM5 == "") ? "error" : Number(dataMap.APR_PSB_AM5); 
			var 카드론잔여var    = (dataMap.APR_PSB_AM8 == undefined || dataMap.APR_PSB_AM8 == "") ? "error" : Number(dataMap.APR_PSB_AM8);  
			var 신용카드여부var = (dataMap.MLI_CCD_YN == undefined || dataMap.MLI_CCD_YN == "") ? "N" : dataMap.MLI_CCD_YN;
			var 카드론동의여부var = (dataMap.MLI_AG_YN == undefined || dataMap.MLI_AG_YN == "") ? "N" : dataMap.MLI_AG_YN;
			
			
			if( 통합한도var == "error" || 현금서비스잔여var == "error" || 카드론잔여var == "error" ){
				
				
				//document.getElementById("errLim").style.display="";
				//document.getElementById("err").innerHTML="데이터 전송이 원활하지 않으니<br/>잠시 후 이용해 주시기 바랍니다.";

			}else{
				
				
				if("Y" == bill_2nd_yn){
					jQuery("#mc_box_area_dt2_login").html("<a href='javascript:void(0);' class='btn_plus' onclick='goMenuLink(\"OPY00350100M.web?schID=pcd&mID=OPY00350100M\"); return false;'>총 이용한도</a>");
		      	  	jQuery("#mc_box_area_em2_login").html("<em>"+getInsComma(Number(통합한도var))+"</em>원");
				}
				
				jQuery("#mc_cash_area_login li:first-child div").html("<strong>이용가능금액</strong>"+getInsComma(Number(현금서비스잔여var))+"<em>원</em>");

				
				if("Y" == 신용카드여부var) {
					if("Y" == 카드론동의여부var) {
						jQuery("#mc_cash_area_login li:nth-child(2) div").html("<strong>이용가능금액</strong>"+getInsComma(Number(카드론잔여var))+"<em>원</em>");
					}
					else {
						jQuery("#mc_cash_area_login li:nth-child(2) div").html("<strong class='txt'>이용동의가 필요합니다.</strong><a href='javascript:void(0);' class='btn_xs btn_ty04' onclick='goMenuLink(\"OPF10200100M.web?schID=pcd&mID=OPF10200100M\"); return false;'>바로가기</a>");
					}
				}
				else {
					//공백처리
					jQuery("#mc_cash_apr_psb_am8").attr("class", "txt");
					jQuery("#mc_cash_apr_psb_am8").html("현재 이용 가능하지 않습니다.");
				}
				
				getCashAndLoanInfo();
			}
			
		}
		
	}
	catch(e)
	{
		//alert("[개발테스트] getExclMemeber :"+e);
	}
}
//=========================================================이용한도 금액 끝

getTitleCard = function(){
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getTitleCardInfo"
	}
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBacTitleCard, AjaxOption);	//callBacMemberAddtion
}

callBacTitleCard = function(dataMap){
	
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! getTitleCard]: "+dataMap.RESPONSE_MESSAGE);
			
		}else{
			var cardName = "";
			
			if(dataMap.CD_PD_KND_NM == undefined || dataMap.CD_PD_KND_NM == null ){
				cardName = "";
			}else{
				cardName = dataMap.CD_PD_KND_NM;
			}
			
			if(dataMap.CD_PD_DESIGN_C == null || dataMap.CD_PD_DESIGN_C =="")
			{
				jQuery("#cardImg").html("<img src='/hana/images/contents/bg_card_thumb.gif' alt='"+dataMap.CD_PD_KND_NM+"' />");
				jQuery("#cardName").html(cardName);
			}
			else
			{
				jQuery("#cardImg").html("<img src='/img/individual/new_card_list/" + dataMap.CD_PD_DESIGN_C + ".gif' onerror='this.src=\"/hana/images/contents/bg_card_thumb.gif\"' alt='"+dataMap.CD_PD_KND_NM+"' />");
				jQuery("#cardName").html(cardName);
			}	
		}
		
	}catch(e){
		//alert("[개발테스트] getTitleCard 에러");
	}
}


getCashAndLoanInfo = function () {
	
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getCashAndLoanInfo"
	}
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBacCashAndLoanInfo, AjaxOption);	
	
}


callBacCashAndLoanInfo = function(dataMap){
	
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! getCashAndLoanInfo]: "+dataMap.RESPONSE_MESSAGE);
			
		}else{
			jQuery("#mc_cash_area_login li:first-child div").html(
				"<p class='money'>"+jQuery("#mc_cash_area_login li:first-child div").html()+"<em class='after'>/</em></p>"+
				"<p class='fees'><em class='after'>연</em><strong>연 이자율</strong>"+dataMap.S_APY_IT_R+"<em>%</em></p>"
			);

			
			if("Y" == 신용카드여부var) {
			  if(dataMap.MRKT_INT_OJ_YN == "Y") {
  				jQuery("#mc_cash_area_login li:nth-child(2) div").html(
  					"<p class='money'>"+jQuery("#mc_cash_area_login li:nth-child(2) div").html()+"<em class='after'>/</em></p>"+
  					"<p class='fees'><em class='after'>연</em><strong>연 이자율</strong><span class='r1'><span class='r2'>"+dataMap.L_LO_IT_R5+"<em>%</em></span>"+dataMap.L_LO_IT_R1+"<em>%</em></span></p>"
  				);
  			} else {
  				if(dataMap.L_LO_IT_R5 == dataMap.L_LO_IT_R1) {
  					jQuery("#mc_cash_area_login li:nth-child(2) div").html(
  						"<p class='money'>"+jQuery("#mc_cash_area_login li:nth-child(2) div").html()+"<em class='after'>/</em></p>"+
  						"<p class='fees'><em class='after'>연</em><strong>연 이자율</strong>"+dataMap.L_LO_IT_R1+"<em>%</em></p>"
  					);
  				} else {
  					jQuery("#mc_cash_area_login li:nth-child(2) div").html(
  						"<p class='money'>"+jQuery("#mc_cash_area_login li:nth-child(2) div").html()+"<em class='after'>/</em></p>"+
  						"<p class='fees'><em class='after'>연</em><strong>연 이자율</strong><span class='r1'><span class='r2'>"+dataMap.L_LO_IT_R5+"<em>%</em></span>"+dataMap.L_LO_IT_R1+"<em>%</em></span></p>"
  					);
  				}
  			}
      }
		}
		
	}catch(e){
		//alert("[개발테스트] getCashAndLoanInfo 에러");
	}
}


getSmartOfferBanner = function(dpid){
	
	AjaxOption.encription = true;
	var reqMap = {
		dpid : dpid
		, CURI : "/OPH30000000N.web"    	  //현재 PAGE URI값. dataMap.URI
		, targetMethod : "smartOffer"
	}
	executeAction("HsciUtilAction.ajax", checkDataMap(reqMap), function(dataMap){ callBacSmartBanner(dataMap,dpid) }, AjaxOption);	
	
}

callBacSmartBanner = function(dataMap, dpid){
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! callBacSmartBanner]: "+dataMap.RESPONSE_MESSAGE);
			
		}else{
			
			var objListData = null;
			var bannerHtml = "";
			var elementCnt = 0;
			
			
			if(dataMap.SMART_OFFER!=null && dataMap.SMART_OFFER!=undefined){
				objListData = dataMap.SMART_OFFER.data;
	    	}
	    	
	    	var WISEGRID_ROWCOUNT = 0;
	        if(objListData!=null && objListData!=undefined){
	        	WISEGRID_ROWCOUNT = objListData.length;
	        }
	        
		    if("WHP_MAIN_BNR_RT_MD" == dpid){	
	        
	        	if (WISEGRID_ROWCOUNT > 0) {
					if(objListData[0].BAN_LINK_URL !=undefined && objListData[0].BAN_ATTACH !=undefined){
						bannerHtml = "";
						bannerHtml +="<a href=\"javascript:goMovePage('"+objListData[0].BAN_LINK_URL+"', '"+objListData[0].CLICKURL+"');\">";
						bannerHtml +="<img src=\""+objListData[0].BAN_ATTACH+"\" alt=\""+objListData[0].BAN_ATTACH_ALT+"\">";
						bannerHtml +="</a>";  
						bannerHtml +="<img src=\"/img/common/rcmd_vw.tmp?"+objListData[0].CLICKURL+"\" alt=\"rcmdVW\" style=\"display:none;\">";
						
						jQuery("#mc_banner_area_login ul li:eq(1)").html(bannerHtml);
					}		       			        	
		        }
	        	
			}else if("WHP_MAIN_BNR_RT_BT" == dpid){	
	        
	        	if (WISEGRID_ROWCOUNT > 0) {
					if(objListData[0].BAN_LINK_URL !=undefined && objListData[0].BAN_ATTACH !=undefined){
						bannerHtml = "";
						bannerHtml +="<a href=\"javascript:goMovePage('"+objListData[0].BAN_LINK_URL+"', '"+objListData[0].CLICKURL+"');\">";
						bannerHtml +="<img src=\""+objListData[0].BAN_ATTACH+"\" alt=\""+objListData[0].BAN_ATTACH_ALT+"\">";
						bannerHtml +="</a>";  
						bannerHtml +="<img src=\"/img/common/rcmd_vw.tmp?"+objListData[0].CLICKURL+"\" alt=\"rcmdVW\" style=\"display:none;\">";     
						
						jQuery("#mc_banner_area_login ul li:eq(2)").html(bannerHtml);
		       		}	        	
		        }	
	       	}else if("WHP_MAIN_BNR_LF_TP" == dpid){	
	        
	        	if (WISEGRID_ROWCOUNT > 0) {
					if(objListData[0].BAN_LINK_URL !=undefined && objListData[0].BAN_ATTACH !=undefined){
						bannerHtml = "";
						bannerHtml +="<a href=\"javascript:goMovePage('"+objListData[0].BAN_LINK_URL+"', '"+objListData[0].CLICKURL+"');\">";
						bannerHtml +="<img src=\""+objListData[0].BAN_ATTACH+"\" alt=\""+objListData[0].BAN_ATTACH_ALT+"\">";
						bannerHtml +="</a>";  
						bannerHtml +="<img src=\"/img/common/rcmd_vw.tmp?"+objListData[0].CLICKURL+"\" alt=\"rcmdVW\" style=\"display:none;\">";     
						
						
						jQuery(".mc02 ul li:eq(0)").html(bannerHtml);
		       		}	        	
		        }	
	       	}else if("WHP_MAIN_BNR_LF_MD" == dpid){	
	        
	        	if (WISEGRID_ROWCOUNT > 0) {
					if(objListData[0].BAN_LINK_URL !=undefined && objListData[0].BAN_ATTACH !=undefined){
						bannerHtml = "";
						bannerHtml +="<a href=\"javascript:goMovePage('"+objListData[0].BAN_LINK_URL+"', '"+objListData[0].CLICKURL+"');\">";
						bannerHtml +="<img src=\""+objListData[0].BAN_ATTACH+"\" alt=\""+objListData[0].BAN_ATTACH_ALT+"\">";
						bannerHtml +="</a>";  
						bannerHtml +="<img src=\"/img/common/rcmd_vw.tmp?"+objListData[0].CLICKURL+"\" alt=\"rcmdVW\" style=\"display:none;\">";     
						
						
						jQuery(".mc02 ul li:eq(1)").html(bannerHtml);
		       		}	        	
		        }	
	       	}else if("WHP_MAIN_BNR_LF_BT" == dpid){	
	        
	        	if (WISEGRID_ROWCOUNT > 0) {
					if(objListData[0].BAN_LINK_URL !=undefined && objListData[0].BAN_ATTACH !=undefined){
						bannerHtml = "";	        			        		
						bannerHtml +="<a href=\"javascript:goMovePage('"+objListData[0].BAN_LINK_URL+"', '"+objListData[0].CLICKURL+"','vip');\">";
						bannerHtml +="<img src=\""+objListData[0].BAN_ATTACH+"\" alt=\""+objListData[0].BAN_ATTACH_ALT+"\">";
						bannerHtml +="</a>";  
						bannerHtml +="<img src=\"/img/common/rcmd_vw.tmp?"+objListData[0].CLICKURL+"\" alt=\"rcmdVW\" style=\"display:none;\">";
						
						
						jQuery(".mc02 ul li:eq(2)").html(bannerHtml);
		       		}	        	
		        }	
	       	}
	        
		}	
		
	}catch(e){
		//alert("[개발테스트] callBacSmartBanner 에러");
		
	}
}

function goMovePage(mUrl, cUrl, type){
	
	fnDoTransaction("/img/common/rcmd_clk.tmp?" + cUrl, "", goMenuLink(mUrl));
}

function goMenuLinkBlank(pageURL){
	openProcessLayer('KO','Y','Y'); // 처리중프로세스바띄운다.
	if(pageURL.indexOf("javascript:") > -1){
		eval(pageURL);
		return;
	}
	spider.menu.moveToLinkBlank(pageURL);
}

getCardServiceMoney = function() {
	
	AjaxOption.encription = true;
	var reqMap = {
		targetMethod : "getCardServiceMoney"
	}
	executeAction("OCM90000000M.ajax", checkDataMap(reqMap), callBacCardServiceMoney, AjaxOption);	
	
}

callBacCardServiceMoney = function(dataMap) {
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! CardServiceMoney]: "+dataMap.RESPONSE_MESSAGE);
			
		}else{
			jQuery("#mc_banner_stt_dt").html("결제일 "+dataMap.STT_DT_FMT+" 기준");
			jQuery("#mc_banner_total_dcam").html(dataMap.TOTAL_DCAM);
			
		}	
		
	}catch(e){
		//alert("[개발테스트] CardServiceMoney 에러");
		
	}
	
	
}

