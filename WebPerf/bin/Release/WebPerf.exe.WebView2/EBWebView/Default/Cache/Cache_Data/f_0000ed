callBacBirthYn = function(dataMap){
	
	try{
		if( null != dataMap.RESPONSE_MESSAGE && undefined != dataMap.RESPONSE_MESSAGE  ) {
			//i18nExtAlert("[개발 테스트 에러! callBacBirthYn]: "+dataMap.RESPONSE_MESSAGE);
		}else{
			
			var birthYn = (undefined == dataMap.TODAY_BIRTH_YN || null == dataMap.TODAY_BIRTH_YN) ? "" : dataMap.TODAY_BIRTH_YN;
			
			if("Y" == birthYn){			
			
				//팝업명 조회 후 오늘 한번만 오픈
	            if("no"!=getCookie("birthPopUp_Html")){
	            	
	            	jQuery('#birthPopUp_Html').popup('show');
	            	jQuery('.audio_play').trigger('click');
            	
	            }
			}
		}
	}catch(e){
		//alert("[개발테스트] callBacBirthYn 에러");
		
	}
	
}
