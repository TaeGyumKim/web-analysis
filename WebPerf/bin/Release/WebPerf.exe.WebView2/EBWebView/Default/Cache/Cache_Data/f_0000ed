<script type="text/javascript">
var isAFCompletion = false; 
var FAM_TYPE_GUBUN = "1";
var isInitSolution = false;
var PSWD_CHANGE_YN;
var CDD_ERROR_YN;
var CDD_IS_YN;
var nullData = "";
var beforeUrl = "";
var beforMid = "";
var beforParam1 = "";
var beforParam2 = "";
var parmWData1;
var parmNData1;
var parmUData1;


var isLogin = false;

pageInit = function() {

// // [OpenWeb] 노현석 솔루선 적용 작업 0217 구 E2E 모듈 삭제();
	duplicateLoginCheckPopup();    	
	
	getSmartOfferBanner('WHP_MAIN_BNR_LF_TP');		
	getSmartOfferBanner('WHP_MAIN_BNR_LF_MD');		
	
	
	if(isLogin){
		goToPage();
	}
	
	
	var promoPopYn = getCookie("promoPopYn");
	if(promoPopYn != "N"){
		jQuery("#promo_pop .caroufredsel_wrapper").css("overflow", "").css("display", "block");
		jQuery('#promo_pop').popup('show');
    
	} 
	
}	

pageonReady();

function initFileter(){
	mcLog();	
	if(!isInitSolution){
		spider.extJS.loadingLayer.show();
		iTracerAF.init(function (data) {
		    jQuery("#ipinsideAfWData2").val(data.wdata);
		    jQuery("#ipinsideAfUKey2").val(data.ukey.replace(/-/g,""));
		    isAFCompletion = true;
		    initEFDSData();
		});
	isInitSolution = true;
	}	
}

